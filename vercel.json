{
  "version": 2,
  "functions": {
    "api/index.py": {
      "runtime": "python3.11",
      "maxDuration": 30,
      "memory": 1024,
      "includeFiles": [
        "requirements.txt",
        "**/*.py"
      ]
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.py"
    }
  ]
}
