{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "functions": {
    "main.py": {
      "runtime": "python3.11",
      "maxDuration": 10,
      "memory": 1024
    }
},
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/main.py"
    }
  ],
  "env": {
    "PYTHONUNBUFFERED": "1"
  }
}
